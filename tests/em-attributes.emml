<em-eml>
    <em-head>
        <em-attributes>
            <em-section background-color="red" />
            <em-all background-color="orange" />
            <em-class name="greenBackground" background-color="green" />
            <em-class name="redText" color="red" />
            <em-class name="bigText" font-size="32px" />
        </em-attributes>
    </em-head>
    <em-body>

        <em-section>
            <em-text>
                em-section background set in head attributes - red<br />
                em-text will inherit orange from el-all.
            </em-text>
        </em-section>

        <em-section background-color="green">
            <em-text class="bigText redText greenBackground">
                em-section backgrund set inline<br />
                em-text with multiple classes defined.
            </em-text>
        </em-section>

        <em-text>
            Body background set using em-all attribute in head
        </em-text>

    </em-body>
</em-eml>